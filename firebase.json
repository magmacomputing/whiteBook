{
	"hosting": [
		{
			"target": "whiteBook",
			"public": "dist/whiteBook",
			"ignore": [
				"firebase.json",
				"**/.*",
				"**/node_modules/**"
			],
			"rewrites": [
				{
					"source": "**",
					"destination": "/index.html"
				}
			],
			"predeploy": "git log -n 1 > dist/whiteBook/git.log"
		}
	]
}